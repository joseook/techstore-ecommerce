{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.go",
      "use": "@vercel/go"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel.go"
    }
  ],
  "env": {
    "GIN_MODE": "release"
  }
}